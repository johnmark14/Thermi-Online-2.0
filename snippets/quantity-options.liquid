<div class="item-quantity">
    <div class="item-actions">
        <div class="qt add">
            <a href="/" @click.prevent="cartDataObject.addQuantity(item, $event)">+</a>
        </div>
        <div class="qt items wide">
            <span>${item.quantity}</span>
        </div>
        <div class="qt minus">
            <a href="/" @click.prevent="cartDataObject.removeQuantity(item, $event)">-</a>
        </div>
    </div>
</div>
